<?xml version="1.0" encoding="utf-8"?>
<manifest
    package="com.example.ancacret.rssfeed"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/news_ic_launcher"
        android:label="@string/app_name"
        android:name=".application.SavedProviderApplication"
        android:theme="@style/AppTheme">
        <!--<activity
            android:name=".MainActivity"
            android:label="@string/app_name">

        </activity>-->
        <activity
            android:name=".ui.activities.FirstActivity" >
        </activity>

        <activity
            android:name=".ui.activities.DetailsPager"
            android:label="">

        </activity>

        <activity
            android:name=".ui.activities.FeedsActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.activities.CategoriesPager"
            android:label="@string/title_activity_categories_pager">
        </activity>

    </application>

</manifest>
